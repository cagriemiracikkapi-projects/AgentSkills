{
  "name": "@cagriemiracikkapi-projects/agentskills-cli",
  "version": "1.0.12",
  "description": "Install AI Agent Skills directly into your project's AI assistant directory (Cursor, Copilot, Claude, Gemini, etc.)",
  "main": "index.js",
  "bin": {
    "agentskills": "./index.js"
  },
  "files": [
    "index.js",
    "README.md",
    ".agents"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "node tests/run-tests.js",
    "validate:content": "node scripts/validate-content.js",
    "prepack": "node -e \"const fs=require('fs'); fs.cpSync('../.agents', '.agents', {recursive:true}); fs.copyFileSync('../README.md', 'README.md')\"",
    "postpack": "node -e \"const fs=require('fs'); fs.rmSync('.agents', {recursive:true, force:true})\"",
    "prepublishOnly": "npm test && npm run validate:content"
  },
  "keywords": [
    "ai",
    "agent",
    "skills",
    "cursor",
    "copilot",
    "claude",
    "gemini",
    "windsurf"
  ],
  "author": "cagriemiracikkapi-projects",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cagriemiracikkapi-projects/AgentSkills.git"
  },
  "homepage": "https://github.com/cagriemiracikkapi-projects/AgentSkills#readme",
  "bugs": {
    "url": "https://github.com/cagriemiracikkapi-projects/AgentSkills/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "inquirer": "^13.3.0"
  }
}
